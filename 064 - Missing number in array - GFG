class Solution {
    int MissingNumber(int array[], int n) {
        // Your Code Here
        //SOrting the array
        Arrays.sort(array);
        
        //traversing through the array
        for(int i=0; i<n-1; i++){
            //checkig if ith element is equal to i+1, if not then returning the i+1
            if(array[i] != i+1)
                return i+1;
        }
        
        /if all the elemtn present from 1 to n-1 then returning the n
        return n;
    }
}

class Solution {

    public long factorial(int N) {
        // Your code here
        //solution one iterative
        /*long res = 1;
        for(int i=1; i<=N; i++){
            res*=i;
        }
        return res;*/
        // Solution factor = new Solution();
        
        //solution two recursive
        
        if(N==0)
        return 1;
        
        return (N*factorial(N-1));
    }
    
    
}

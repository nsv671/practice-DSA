class Solution{
    //Function to check if two arrays are equal or not.
    public static boolean check(long A[],long B[],int N)
    {
        //Your code here
        //Sorting the Array
        Arrays.sort(A);
        Arrays.sort(B);
        
        //comparing both the array 
        for(int i=0; i<N; i++){
        
            // if the element is different then return false
            if(A[i]!=B[i])
                return false;
        }
        return true;
    }
}
